{
  "manifest_version": 2,
  "name": "5000兆単位系コンバータ",
  "version": "1.1.0",
  "description": "右クリックメニューから単位付き数値を5000兆単位系に変換します。",
  "author": "uhyo",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "<all_urls>",
    "activeTab",
    "background",
    "contextMenus"
  ],
  "background": {
    "scripts": ["dist/bundle.js"]
  }
}
